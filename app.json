{
  "name": "gofinances",
  "displayName": "gofinances",
  "githubUrl": "https://github.com/rafaeld3v/gofinances",
  "expo": {
    "name": "gofinances",
    "slug": "gofinances",
    "scheme": "gofinances",
    "version": "1.2.0",
    "versionCode": 2,
    "orientation": "portrait",
    "originalFullName": "@username/slug",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4124B2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rafaeld3v.gofinances",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rafaeld3v.gofinances",
      "config": {
        "keystorePath": "android/app/upload-keystore.jks",
        "keystoreAlias": "upload"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2c94296f-71b4-43a4-be78-da9457ddfbc8"
      }
    },
    "plugins": ["expo-apple-authentication"]
  }
}
